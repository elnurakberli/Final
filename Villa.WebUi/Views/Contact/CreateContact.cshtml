@model CreateContactDto
@{
    ViewData["Title"] = "New Contact information Add Page";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    <label>Map url</label>
    <input type="text" asp-for="MapUrl" class="form-control" />
    <br />
    <label>Phone </label>
    <input type="text" asp-for="Phone" class="form-control" />
    <br />
    <label>Email</label>
    <input type="text" asp-for="Email" class="form-control" />
    <br />
    <button class="btn btn-outline-dark">Save</button>
</form>


