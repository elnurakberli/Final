@model UpdateFeatureDto
@{
    ViewData["Title"] = "Highlights information Update Page";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="Id" />
    <label>Picture Url </label>
    <input type="text" asp-for="ImageUrl" class="form-control" />
    <br />
    <label>Title</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Subtitle</label>
    <input type="text" asp-for="SubTitle" class="form-control" />
    <br />
    <label>Square</label>
    <input type="text" asp-for="Square" class="form-control" />
    <br />
    <label>Contract</label>
    <input type="text" asp-for="Contract" class="form-control" />
    <br />
    <label>Payment</label>
    <input type="text" asp-for="Payment" class="form-control" />
    <br />
    <label>Security Situation</label>
    <input type="text" asp-for="Safety" class="form-control" />
    <br />
    <button class="btn btn-outline-dark">Update</button>

</form>